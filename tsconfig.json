{
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "strict": true,
    "target": "ES2020",
    "preserveSymlinks": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
  ]
}